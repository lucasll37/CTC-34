digraph G {    
    rankdir=LR;
    size="10,5";
    
    // Designa os estilos
    node [shape = circle, fontsize="16"];

    // Ponto inicial que aponta para o estado inicial
    ini [shape=point];
    ini -> q0;

    // States
    q0 [label="q0"];
    q1 [label="q1"];
    q2 [label="q2"];
    q3 [label="q3"];
    q4 [label="q4"];
    q5 [label="q5"];
    q_accept [label="q_accept", fillcolor=lightgray, shape=doublecircle];

    // Transitions
    q0 -> q0 [label="x/x,R"];
    q0 -> q1 [label="y/a,R"];
    q0 -> q_accept [label="∪/∪,R"];

    q1 -> q3 [label="x/y,L"];
    q1 -> q1 [label="y/y,R"];
    q1 -> q1 [label="a/a,R"];
    q1 -> q2 [label="∪/∪,L"];

    q2 -> q4 [label="a/y,R"];
    q2 -> q2 [label="∪/∪,L"];
    q2 -> q2 [label="y/y,L"];
    q2 -> q2 [label="x/x,L"];

    q3 -> q5 [label="a/x,-"];
    q3 -> q3 [label="∪/∪,L"];
    q3 -> q3 [label="x/x,L"];
    q3 -> q3 [label="y/y,L"];

    q4 -> q4 [label="y/y,R"];
    q4 -> q_accept [label="∪/∪,-"];

    q5 -> q0 [label="∪/∪,R"];
    q5 -> q0 [label="x/x,R"];
    q5 -> q0 [label="y/y,R"];
}
